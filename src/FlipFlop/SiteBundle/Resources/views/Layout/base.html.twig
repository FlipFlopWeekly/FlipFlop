{% set _content = block('content') %}

<!DOCTYPE html>
<html>
    <head>
        {% include 'FlipFlopSiteBundle:Layout:meta.html.twig' %}
        <title>
        	{% block title %}
        		{% include 'FlipFlopSiteBundle:Layout:title.html.twig' %}
        	{% endblock %}
       	</title>
        {% include 'FlipFlopSiteBundle:Layout:css.html.twig' %}
        {% include 'FlipFlopSiteBundle:Layout:js.html.twig' %}
    </head>

    <body >
    	<div id="wrap">
    		<div id="wrap-in">
		    	{% block header %}
		       		{%  include 'FlipFlopSiteBundle:Layout:header.html.twig' %}
		       	{% endblock %}
		       	<div id="main">
			       	{% block main %}
			       		<div id="carrousel">
							{%  include 'FlipFlopSiteBundle:Layout:carrousel.html.twig' %}
						</div>
			       	{% endblock %}
		       	</div>
	       	</div>
	        {% if _content is not empty %}
	            {{ _content|raw }}
	        {% endif %}
        </div>
    </body>
</html>